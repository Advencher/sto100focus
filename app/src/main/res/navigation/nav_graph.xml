            app:startDestination="@id/menuDrawable">

    <activity android:id="@+id/mapsActivity" android:name="focus.application.sto100.MapsActivity"
              android:label="activity_maps" tools:layout="@layout/activity_maps"/>
    <activity android:id="@+id/menuDrawable" android:name="focus.application.sto100.MainNavMenu"
              android:label="content_menu_drawable" tools:layout="@layout/content_menu_drawable"/>
    <action android:id="@+id/action_global_mapsActivity" app:destination="@+id/mapsActivity"
            app:enterAnim="@anim/nav_default_enter_anim" app:exitAnim="@anim/nav_default_exit_anim"
